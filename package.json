{
  "name": "lightpulse",
  "version": "0.0.1",
  "description": "An RxJS based component library",
  "main": "index.js",
  "author": "Juan Francisco Marino",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lint": "./node_modules/.bin/tslint -c tslint.json index.ts src/**/*.ts",
    "start": "./node_modules/.bin/parcel index.js",
    "build": "./node_modules/.bin/parcel build index.js",
    "test": "./node_modules/.bin/ava src/test/*.spec.js"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.3",
    "ava": "^0.24.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "jasmine-core": "^2.8.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.1",
    "karma-typescript": "^3.0.9",
    "parcel-bundler": "^1.4.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "rxjs": "^5.5.6",
    "snabbdom": "^0.7.1",
    "whatwg-fetch": "^2.0.3"
  },
  "ava": {
    "files": [
      "src/test/*.js"
    ],
    "failFast": true,
    "failWithoutAssertion": true,
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
